################################################
## JMEScript v0.2.0: ejemplo de script
## lectura de coordenadas bidimensionales
################################################

puntos := []

# introducir INTRO para finalizar
mientras verdadero inicio:
	leer x msj 'x? '
	si indefinido(['x']) inicio:
		romper
	fin
	leer y msj 'y? '
	si indefinido(['y']) inicio:
		romper
	fin
	puntos ;= [x,y]
fin

devolver puntos